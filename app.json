{
	"id": "com.sonos",
	"version": "2.2.0",
	"compatibility": ">=1.2.0",
	"name": {
		"en": "Sonos"
	},
	"author": {
		"name": "Athom B.V.",
		"website": "https://www.athom.com"
	},
	"category": [ "music" ],
	"description": {
		"en": "Sonos for Homey"
	},
	"images": {
		"large": "/assets/images/large.png",
		"small": "/assets/images/small.png"
	},
	"media": [ "play", "getPlaylists", "getPlaylist" ],
	"drivers": [
		{
			"id": "sonos",
			"name": {
				"en": "Sonos"
			},
			"images": {
				"large": "/drivers/sonos/assets/images/large.png",
				"small": "/drivers/sonos/assets/images/small.png"
			},
			"class": "speaker",
			"capabilities": [ "speaker_playing", "speaker_prev", "speaker_next", "volume_set", "volume_mute" ],
			"pair": [
				{
					"id": "list_devices",
					"template": "list_devices",
					"navigation": {
						"next": "add_devices"
					}
				},
				{
					"id": "add_devices",
					"template": "add_devices"
				}
			]
		}
	]
}